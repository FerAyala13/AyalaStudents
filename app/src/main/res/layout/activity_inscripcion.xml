<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/pastel"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Inscripcion">

    <TextView
        android:layout_width="323dp"
        android:layout_height="42dp"
        android:text="INSCRIPCION DE ESTUDIANTE"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"></TextView>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:src="@drawable/registraruser"
        app:layout_constraintBottom_toTopOf="@+id/Nomins"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"></ImageView>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="94dp"
        android:layout_height="49dp"
        android:layout_marginStart="16dp"
        android:text="Nombres"
        android:background="@color/purple_200"
        android:textColor="@color/white"
        android:textSize="16dp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="242dp"></TextView>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="94dp"
        android:layout_height="49dp"
        android:layout_marginStart="16dp"
        android:text="Apellidos"
        android:background="@color/purple_200"
        android:textColor="@color/white"
        android:textSize="16dp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="316dp"></TextView>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="94dp"
        android:layout_height="49dp"
        android:layout_marginStart="16dp"
        android:text="Edad"
        android:background="@color/purple_200"
        android:textColor="@color/white"
        android:textSize="16dp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="389dp"></TextView>

    <TextView
        android:id="@+id/textView8"
        android:layout_width="94dp"
        android:layout_height="49dp"
        android:layout_marginStart="16dp"
        android:text="Direccion"
        android:background="@color/purple_200"
        android:textColor="@color/white"
        android:textSize="16dp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="558dp"></TextView>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="94dp"
        android:layout_height="49dp"
        android:layout_marginStart="16dp"
        android:text="Fecha de Nacimiento"
        android:background="@color/purple_200"
        android:textColor="@color/white"
        android:textSize="16dp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="467dp"></TextView>

    <EditText
        android:id="@+id/Nomins"
        android:layout_width="267dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toTopOf="@+id/Apeins"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/Apeins"
        android:layout_width="267dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toTopOf="@+id/Edad"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/Edad"
        android:layout_width="267dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="28dp"
        app:layout_constraintBottom_toTopOf="@+id/Fechanac"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/Fechanac"
        android:layout_width="267dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="40dp"
        app:layout_constraintBottom_toTopOf="@+id/Direccion"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/Direccion"
        android:layout_width="267dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="124dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>

